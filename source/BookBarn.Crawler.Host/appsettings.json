{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "CrawlerHostConfiguration": {
    "MaxConcurrentCrawlers": 2,
    "IdlePollingIntervalMilliseconds": 5000,
    "FailedCrawlerRetryCount": 2
  },
  "RequestThrottleOptions": {
    "MaxConcurrentRequests": 2,
    "MaxQueuedRequests": 1000,
    "IntervalSeconds": 1,
    "PartitionIdleTTLSeconds": 600
  },
  "BookBarnApi": {
    "ServiceEndpoint": "http://localhost:5230",
    "ServiceApiKey": ""
  },
  "AllowedHosts": "*"
}
